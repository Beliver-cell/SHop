{
  "version": 2,
  "builds": [
    {
      "src": "backendv3/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "backendv3/**"
      }
    },
    {
      "src": "frontendv3/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontendv3/dist"
      }
    },
    {
      "src": "admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "admin/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backendv3/server.js"
    },
    {
      "src": "/admin-fantasy-luxe-panel-working/(.*)",
      "dest": "admin/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontendv3/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
